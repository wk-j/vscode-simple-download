{
    "name": "vscode-simple-download",
    "displayName": "Simple Downloader",
    "description": "Download http file into vscode workspace",
    "version": "18.3.1",
    "publisher": "wk-j",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/wk-j/vscode-simple-download"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.simpleDownload.showQuickPick",
                "title": "Download (Simple Downloader)"
            }
        ],
        "configuration": {
            "title": "Simple downloader configuration",
            "type": "object",
            "properties": {
                "simpleDownload": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "url": {
                                "type": "string"
                            },
                            "target": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^5.2.0",
        "typescript": "^2.0.3",
        "vscode": "^1.1.21"
    },
    "dependencies": {}
}
